<% content_for :breadcrumb do %>
  <h1 class="text-2xl font-semibold"><%= Foto.model_name.human %></h1>
<% end %>
<div class="py-5">
      <p>
      <strong><%= Foto.human_attribute_name :nome_arquivo %>:</strong>
      <%= @foto.nome_arquivo %>
    </p>
      <p>
      <strong><%= Foto.human_attribute_name :token %>:</strong>
      <%= @foto.token %>
    </p>
      <p>
      <strong><%= Foto.human_attribute_name :servico %>:</strong>
      <%= @foto.servico %>
    </p>
      <p>
      <strong><%= Foto.human_attribute_name :titulo %>:</strong>
      <%= @foto.titulo %>
    </p>
  </div>

<%= link_to(t('helpers.links.back'), fotos_path, class: 'px-4 py-2 text-sm text-white rounded-md bg-primary hover:bg-primary-dark focus:outline-none focus:ring focus:ring-primary focus:ring-offset-1 focus:ring-offset-white dark:focus:ring-offset-dark') %>
<%= link_to t('helpers.links.edit'), edit_foto_path(@foto), class: 'px-4 py-2 text-sm text-white rounded-md bg-primary hover:bg-primary-dark focus:outline-none focus:ring focus:ring-primary focus:ring-offset-1 focus:ring-offset-white dark:focus:ring-offset-dark' %>