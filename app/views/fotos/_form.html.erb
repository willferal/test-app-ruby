<div class="bg-white rounded-md dark:bg-darker relative p-6 mb-20">
  <%= form_for(@foto, html: { class: 'space-y-4' }) do |f| %>
    <%= render partial: 'shared/errors_messages', locals: { model: @foto } %>
    <div class="mb-4">
      <%= f.label :nome_arquivo, class: 'control-label' %>
      <%= f.text_field :nome_arquivo, class: 'control-field validation', placeholder: 'Nome_arquivo' %>
    </div>
    <div class="mb-4">
      <%= f.label :token, class: 'control-label' %>
      <%= f.text_field :token, class: 'control-field validation', placeholder: 'Token' %>
    </div>
    <div class="mb-4">
      <%= f.label :servico, class: 'control-label' %>
      <%= f.text_field :servico_id, class: 'control-field validation', placeholder: 'Servico' %>
    </div>
    <div class="mb-4">
      <%= f.label :titulo, class: 'control-label' %>
      <%= f.text_field :titulo, class: 'control-field validation', placeholder: 'Titulo' %>
    </div>
    <div class="actions">
      <%= link_to t('helpers.links.cancel'), fotos_path, class: 'btn-secondary' %>
      <%= f.submit nil, class: 'btn-primary' %>
    </div>
  <% end %>
</div>